{% extends 'base.jinja' %}

{%block head%}
<script src="{{ url_for('static', filename='get-log-lines.js') }}"></script>
<script>
function display_filters(){
   var x = document.getElementById("dropdown_filters_container");
  if (x.style.display === "none") {
    x.style.display = "block";
  } else {
    x.style.display = "none";
  }
}

</script>
<title>
Team TABLE Log Analysis
</title>
{%endblock%}

